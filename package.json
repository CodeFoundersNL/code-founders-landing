{
  "name": "code_founders_landing",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "npm run tinacms && astro build",
    "build:watch": "npm run build && concurrently \"chokidar '{src,public}/**/*' -c 'rimraf dist && npm run build'\" \"wds --root-dir dist\"",
    "build:preview": "DEPLOY_PREVIEW=true npm run build",
    "build:start": "npm run build && cd dist && http-server -o -c-1",
    "build:start:preview": "npm run build:preview && cd dist && http-server -o -c-1",
    "preview": "astro preview"
  },
  "devDependencies": {
    "@astrojs/renderer-lit": "^0.3.1",
    "@astrojs/renderer-react": "^0.4.1",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@web/dev-server": "^0.1.29",
    "astro": "^0.22.20",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^7.0.0",
    "http-server": "^14.1.0",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.66.1"
  },
  "dependencies": {
    "@divriots/rev": "^0.1.10",
    "@tinacms/cli": "^1.3.2",
    "lit": "^2.1.2",
    "tinacms": "^1.3.2"
  },
  "prettier": {
    "tabWidth": 2,
    "singleQuote": true,
    "arrowParens": "always",
    "trailingComma": "all"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
